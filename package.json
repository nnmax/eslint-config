{
  "devDependencies": {
    "@types/prettier": "^2.7.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.24.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": ">=6",
    "lint-staged": ">=10",
    "prettier": "^2.7.1"
  },
  "lint-staged": {
    "*.{js,md,yml,yaml,json}": "prettier --write",
    "*.{js,ts}": "eslint --fix"
  },
  "name": "@nnmax/eslint-config",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nnmax/eslint-config.git"
  },
  "scripts": {
    "bump": "bumpp package.json packages/*/package.json --commit --push --tag",
    "eslint": "eslint .",
    "eslint:fix": "yarn eslint --fix",
    "postinstall": "husky install",
    "prettier": "prettier --write .",
    "prettier:ci": "prettier --check .",
    "publish": "yarn workspaces foreach --all --no-private --parallel npm publish --access public",
    "release": "yarn bump && yarn publish"
  },
  "version": "1.0.0-beta.0",
  "workspaces": [
    "packages/*"
  ]
}
